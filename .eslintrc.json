{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "warn",
    "prettier/prettier": "warn",
    "semi": "off",
    "react/no-unknown-property": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
